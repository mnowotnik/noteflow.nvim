{
  "name": "noteflow-preview",
  "version": "0.0.1",
  "main": "run.js",
  "scripts": {
    "dev": "concurrently \"rollup -c -w\" \"nodemon --watch src -e ts --exec npm run --silent start\"",
    "build": "rollup -c",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "start": "ts-node -O '{\"module\":\"commonjs\"}' src/server.ts",
    "debug": "DEBUG_NOTEFLOW=1 ts-node -O '{\"module\":\"commonjs\"}' src/server.ts"
  },
  "author": "Micha≈Ç Nowotnik",
  "license": "MIT",
  "repository": "github:mnowotnik/noteflow.nvim",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-typescript": "^8.2.1",
    "@tsconfig/svelte": "^1.0.10",
    "@types/node": "^14.14.41",
    "@types/pino": "^6.3.7",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/tmp": "^0.2.0",
    "concurrently": "^6.0.2",
    "eslint": "^7.24.0",
    "nodemon": "^2.0.7",
    "rollup": "^2.45.2",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-svelte": "^7.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@shd101wyy/mume": "^0.6.6",
    "cheerio": "^1.0.0-rc.6",
    "diffhtml": "^1.0.0-beta.18",
    "fastify": "^3.14.2",
    "fastify-static": "^4.0.1",
    "fastify-websocket": "^3.1.0",
    "get-port": "^5.1.1",
    "momentum-slider": "^0.0.8",
    "pino": "^6.11.3",
    "svelte": "^3.37.0",
    "tmp": "^0.2.1",
    "vscode-languageserver": "^7.0.0",
    "vscode-languageserver-textdocument": "^1.0.1"
  }
}
